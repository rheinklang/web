{
	"name": "rheinklang-kollektiv",
	"version": "0.9.182-rc.1",
	"scripts": {
		"ng": "ng",
		"prestart": "run-s create:*",
		"start": "ng serve --host 127.0.0.1 --disable-host-check",
		"create:env": "npx npm-run-all create:env:*",
		"create:env:dev": "node bin/create-env",
		"create:env:prod": "node bin/create-env --environment=production",
		"create:version": "node bin/create-version",
		"create:sitemap": "npx sitemap-generator-cli https://rheinklang-festival.ch -f ./src/server/sitemap.xml",
		"prebuild": "run-s create:*",
		"build": "ng build",
		"test": "echo 'No tests defined'",
		"lint": "ng lint",
		"commit": "git-cz",
		"preproduction": "run-s create:*",
		"production": "ng build --configuration=production --aot=true --buildOptimizer=true --optimization=true",
		"postproduction": "node bin/copy-files",
		"pipeline": "npx npm-run-all pipeline:*",
		"pipeline:version": "npm run create:version",
		"pipeline:env": "npm run create:env",
		"pipeline:build": "npm run production",
		"prettier": "prettier --write \"**/*.*(js|jsx|ts|tsx|json|md)\"",
		"deploy:development": "node deploy.js --tag=development",
		"deploy:production": "node deploy.js --tag=production",
		"deploy:local:development": "node -r dotenv/config deploy.js --tag=development",
		"deploy:local:production": "node -r dotenv/config deploy.js --tag=production"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~8.2.0",
		"@angular/cdk": "~8.2.0",
		"@angular/common": "~8.2.0",
		"@angular/compiler": "~8.2.0",
		"@angular/core": "~8.2.0",
		"@angular/forms": "~8.2.0",
		"@angular/material": "~8.2.0",
		"@angular/material-moment-adapter": "~8.2.0",
		"@angular/platform-browser": "~8.2.0",
		"@angular/platform-browser-dynamic": "~8.2.0",
		"@angular/router": "~8.2.0",
		"@glidejs/glide": "3.4.1",
		"@ngneat/content-loader": "4.1.0",
		"apollo-angular": "1.8.0",
		"apollo-angular-link-http": "1.9.0",
		"apollo-cache-inmemory": "1.6.5",
		"apollo-client": "2.6.8",
		"apollo-link": "1.2.13",
		"apollo-link-error": "1.1.12",
		"graphql": "^14.0.0",
		"graphql-tag": "2.10.1",
		"lazysizes": "5.2.0",
		"masonry-layout": "4.2.2",
		"microtip": "0.2.2",
		"moment": "2.24.0",
		"normalize.css": "8.0.1",
		"rxjs": "~6.5.0",
		"svgxuse": "1.2.6",
		"swiper": "5.3.6",
		"tslib": "~1.10.0",
		"ua-parser-js": "0.7.21",
		"zone.js": "~0.10.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.803.22",
		"@angular/cli": "~8.3.22",
		"@angular/compiler-cli": "~8.2.0",
		"@angular/language-service": "~8.2.0",
		"@types/fs-extra": "8.1.0",
		"@types/googlemaps": "3.39.2",
		"@types/jasmine": "~3.3.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/masonry-layout": "4.2.1",
		"@types/node": "~8.9.4",
		"@types/ol": "6.1.0",
		"@types/swiper": "5.2.1",
		"@types/ua-parser-js": "0.7.33",
		"chalk": "2.4.2",
		"codelyzer": "^5.0.0",
		"dotenv": "8.1.0",
		"ftp-deploy": "2.3.3",
		"git-cz": "3.2.1",
		"git-describe": "4.0.4",
		"husky": "4.2.3",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.1.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.0",
		"lint-staged": "10.0.7",
		"npm-run-all": "4.1.5",
		"ora": "3.4.0",
		"prettier": "1.18.2",
		"protractor": "~5.4.0",
		"ts-node": "~7.0.0",
		"tslint": "~5.15.0",
		"typescript": "~3.5.3"
	},
	"config": {
		"commitizen": {
			"path": "git-cz"
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "npm run lint"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,md}": ["prettier --write", "git add"]
	}
}
